{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "no-shadow": "off",
    "no-underscore-dangle": "off",
    "import/no-extraneous-dependencies": "off",
    "linebreak-style": "off",
    "comma-dangle": "off",
    "eol-last": "off",
    "semi": "off",
    "no-trailing-spaces": "off",
    "quotes": "off",
    "indent": "off",
    "no-unused-vars": "off",
    "space-before-function-paren": "off",
    "no-undef": "off",
    "no-restricted-globals": ["error", "sef"],
    "import/no-named-as-default": "off",
    "max-len": "off",
    "no-var": "off",
    "prefer-const": "off",
    "no-param-reassign": "off",
    "no-cond-assign": "off",
    "no-sequences": "off",
    "no-return-assign": "off",
    "no-return-await": "off",
    "no-await-in-loop": "off",
    "eqeqeq": "off",
    "vars-on-top": "off",
    "no-func-assign": "off",
    "no-multi-assign": "off",
    "no-continue": "off",
    "no-nested-ternary": "off",
    "no-proto": "off",
    "no-new-func": "off",
    "no-unreachable": "off",
    "no-use-before-define": "off",
    "no-restricted-syntax": "off",
    "guard-for-in": "off",
    "new-cap": "off",
    "import/no-amd": "off",
    "import/prefer-default-export": "off",
    "func-names": "off",
    "class-methods-use-this": "off",
    "block-scoped-var": "off",
    "prefer-arrow-callback": "off",
    "prefer-destructuring": "off",
    "no-unused-expressions": "off",
    "global-require": "off",
    "prefer-spread": "off",
    "no-promise-executor-return": "off",
    "consistent-return": "off",
    "no-void": "off",
    "no-mixed-operators": "off",
    "prefer-regex-literals": "off",
    "no-fallthrough": "off",
    "no-loop-func": "off",
    "no-useless-escape": "off",
    "default-case": "off",
    "prefer-rest-params": "off",
    "max-classes-per-file": "off",
    "no-empty": ["error", { "allowEmptyCatch": true }],
    "no-bitwise": "off",
    "no-redeclare": "off",
    "no-plusplus": "off"
  }
}